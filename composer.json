{
    "name": "valigara/amazon-fba-shipping-service",
    "description": "Amazon FBA Shipping Service implementation for fulfilling orders",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valigara Team",
            "email": "dev@valigara.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "vlucas/phpdotenv": "^5.5",
        "symfony/http-foundation": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.25"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
